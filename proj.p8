__lua__
--main
function _init()
	state="start"
	putbacks={}
	setup()
	
end

function _update()
	if state=="main" then
		lvl_update()
		player_update()
		
	elseif state=="start" then
		if btnp(❎) then
			state="main"
		end
	elseif state=="over" then
		if btnp(❎) then
			setup()
			state="start"
		end
	end
	
	lvl_update()
end

function _draw()
	cls()
	
	if state=="main" then
		draw_game()
		pickup()
		draw_cam()
		draw_ui()
	elseif state=="start" then
		menu()
	elseif state=="over" then
		gameover()
	end

end 


function setup()
	player_init()
	lvl_init()
	
	--reset taken obj
	for i in all(putbacks) do
		mset(i.x,i.y,i.s)
	end
	
	putbacks={}
	
end
-->8
--player
function player_init()
	--change player pos to diff areas
	if tl==0 then
		px,py=63,63
	elseif tl==1 then
		px,py=63,250
	elseif tl==2 then
		px,py=482,55
	elseif tl==3 then
		px,py=469,297
	end

	flp = false
	health = 5
	cx,cy = 0,0
end

function player_update()
	local lx,ly=px,py --save curr pos

 if btn(⬅️) then
	 px-=1
		flp = true
 elseif btn(➡️) then
  px+=1
  flp = false
 end
	
 if btn(⬆️) then
  py-=1
 elseif btn(⬇️) then
  py+=1
 end	
 
	local ptx,pty=(px+4)/8,(py+4)/8
	
	--wall collision
 if fget(mget(ptx,pty),0)==true then
 	px=lx
 	py=ly
 end
 
 if health<=0 then
		state="over"
		win=false
	end
		
end

function pickup()

	local ptx=(px+4)/8
	local pty=(py+4)/8
		
	sprover=mget(ptx,pty)
	
	--heart
	if(sprover == 9) then
		mset(ptx,pty,13)
		--save pos in table
		add(putbacks,{x=ptx,y=pty,s=9})
		health += 2  
	end
	
	--nail
	if(sprover == 8) then
		mset(ptx,pty,13)
		add(putbacks,{x=ptx,y=pty,s=8})
		health -= 5
	end
	
	--wire
	if(sprover == 6) then
		mset(ptx,pty,13)
		add(putbacks,{x=ptx,y=pty,s=6})
		wire += 1
		tstart=true
	end
	
	--metal
	if(sprover == 7) then
		mset(ptx,pty,13)
		add(putbacks,{x=ptx,y=pty,s=7})
		metal += 1
		tstart=true
	end
	
end



-->8
--levels
function lvl_init()
	tl=0
	bc=10 --bar colour
	bg=1 --bg colour
	lvl_reset()
end

function lvl_update()
	--for each tl
	if tl==0 then
		goalm,goalw=1,1
		bg=1
	elseif tl==1 then
		goalm,goalw=2,2
		bg=5
	elseif tl==2 then
		goalm,goalw=3,3
		bg=3
	elseif tl==3 then
		goalm,goalw=3,3
		bg=13
	end
	
	--check tl goals
 if metal>=goalm and wire>=goalw then
 	lvlgoal=true 	
 end
 
 --if goal is met, change tl
	if lvlgoal==true then
			tl+=1
			lvlgoal=false
			lvl_reset()
			player_init()
			
			if tl>3 then
 			state="over"
 	 	win=true
 		end
 		
	end
	
	--tl timer
	if tstart==true then
		if ttimer<= 0 then
			ttimer=0
			
			if lvlgoal==false then
				state="over"
				win=false
			end
				
		else
			ttimer-=.001
		end 
		
		--change bar colour
		if ttimer>ttimer*.3 then	bc=8	end
		if ttimer>ttimer*.5 then bc=9	end
		if ttimer>ttimer*.7 then	bc=10	end	
	end
	
end 

function lvl_reset()
	ttimer=30
	tstart=false
	lvlgoal=false
	metal,wire=0,0
end 


	

-->8
--scenes + ui
function menu()
	cls()
	camera(0,0)
	print("press ❎ to start",35,50,7)
	player_init() --reset player
end

function draw_ui()
	print("health:".. health,cx,cy,7)
	print("wires:".. wire,cx,cy+7,7)
	print("metal:".. metal,cx,cy+14,7)
	
	--progress bar
	bx,by=cx+128,cy
	print("time: ".. ttimer,cx+78,cy+8,bc)
	rectfill(bx,by,bx-ttimer,by+5,bc)

	print("timeline:"..tl,cx,cy+21,7)
	print("goal:"..goalm..","..goalw,cx+45,cy,7)
end

function draw_cam()
	cx=px-60
	cy=py-60
	
	camera(cx,cy)
end

function draw_game()
		map(0,0)		
		spr(1,px,py,1,1,flp)
		
		--change bg col to map bg
		pal(15,bg)
end

function gameover()
	cls()
	camera(0,0)
	print("gameover!",50,40)
	
	if win then
		print("you win!",50,50)
	elseif win!=true then
		print("you lose!",50,50)
	end
	
	print("press ❎ to play again",25,60)

end
