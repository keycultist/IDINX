--levels
function lvlin()
	tl=0
	bc=10 --bar colour
	bg=1 --bg colour
	lvl_reset()
end

function lvlupdate()
	--for each tl
	if tl==0 then
		goalm,goalw=1,1
		bg=1
	elseif tl==1 then
		goalm,goalw=2,2
		bg=5
	elseif tl==2 then
		goalm,goalw=3,3
		bg=3
	elseif tl==3 then
		goalm,goalw=3,3
		bg=13
	end
	
	--check tl goals
 if metal>=goalm and wire>=goalw then
 	lvlgoal=true 	
 end
 
 --if goal is met, change tl
	if lvlgoal==true then
			tl+=1
			lvlgoal=false
			lvl_reset()
			player_init()
			
			if tl>3 then
 			state="over"
 	 	win=true
 		end
 		
	end
	
	--tl timer
	if tstart==true then
		if ttimer<= 0 then
			ttimer=0
			
			if lvlgoal==false then
				state="over"
				win=false
			end
				
		else
			ttimer-=.001
		end 
		
		--change bar colour
		if ttimer>ttimer*.3 then	bc=8	end
		
		if ttimer>ttimer*.5 then bc=9	end
		
		if ttimer>ttimer*.7 then	bc=10	end	
	end
end 

function lvl_reset()
	ttimer=30
	tstart=false
	lvlgoal=false
	metal,wire=0,0
end 

