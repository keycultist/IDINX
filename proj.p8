--player
function player_init()
		--change player pos to diff areas
	if tl==0 then
		px,py=63,63
	elseif tl==1 then
		px,py=63,250
	elseif tl==2 then
		px,py=482,55
	elseif tl==3 then
		px,py=469,297
	end

	flp = false
	health = 5
	cx,cy = 0,0
end

function player_update()
	local lx,ly=px,py --save curr pos

 if btn(⬅️) then
	 px-=1
		flp = true
 elseif btn(➡️) then
  px+=1
  flp = false
 end
	
 if btn(⬆️) then
  py-=1
 elseif btn(⬇️) then
  py+=1
 end	
 
	local ptx,pty=(px+4)/8,(py+4)/8
--wall collision
 if fget(mget(ptx,pty),0)==true then
 	px=lx
 	py=ly
 end
 
 if health<=0 then
		state="over"
		win=false
 end	
end


function pickup()

	local ptx=(px+4)/8
	local pty=(py+4)/8
		
	sprover=mget(ptx,pty)
	
	--heart
	if(sprover == 9) then
		mset(ptx,pty,13)
		--save pos in table
		add(putbacks,{x=ptx,y=pty,s=9})
		health += 2  
	end
	
	--nail
	if(sprover == 8) then
		mset(ptx,pty,13)
		add(putbacks,{x=ptx,y=pty,s=8})
		health -= 5
	end
	
	--wire
	if(sprover == 6) then
		mset(ptx,pty,13)
		add(putbacks,{x=ptx,y=pty,s=6})
		wire += 1
		tstart=true
	end

	--metal
	if(sprover == 7) then
		mset(ptx,pty,13)
		add(putbacks,{x=ptx,y=pty,s=7})
		metal += 1
		tstart=true
	end
	
	
end

