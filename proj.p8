__lua__
--main
function _init()
	state="start"
	putbacks={}
	setup()
end

function _update()
	if state=="main" then
		lvl_update()
		player_update()
	elseif state=="start" then
		if btnp(❎) then
			state="main"
		end
	elseif state=="over" then
		if btnp(❎) then
			setup()
			state="start"
		end
	end

	lvl_update()
end

function _draw()
	cls()
	
	if state=="main" then
		draw_game()
		pickup()
		draw_cam()
		draw_ui()
	elseif state=="start" then
		menu()
	elseif state=="over" then
		gameover()
	end

end 


function setup()
	player_init()
	lvl_init()

	--reset taken obj
	for i in all(putbacks) do
		mset(i.x,i.y,i.s)
	end
	
	putbacks={}
end
